<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>iptvboss-alpha</Name>
  <Beta>False</Beta>
  <Category>Tools:Utilities</Category>
  <Overview>iptvboss-alpha</Overview>
  <Description>
	iptvboss-alpha
  </Description>
  <Change></Change>
  <Support></Support>
  <Registry>https://registry.hub.docker.com/u/masterfwadmin/alpha/</Registry>
  <Repository>masterfwadmin/alpha</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>57000</HostPort> 
        <ContainerPort>5800</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
    </Networking>
    <Environment>
      <Variable>
        <Value>99</Value>
        <Name>USER_ID</Name>
        <Mode/>
      </Variable>
      <Variable>
        <Value>100</Value>
        <Name>GROUP_ID</Name>
        <Mode/>
      </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/iptvboss-alpha</HostDir>
      <ContainerDir>/app</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:57000]/</WebUI>
  <Banner></Banner>
  <Icon>https://raw.githubusercontent.com/masterfwadmin/iptvboss/master/src/iptvboss.png</Icon>
  <Config Name="Varible JSPORTS_USERNAME" Target="JSPORTS_USERNAME" Default="username" Mode="" Description="Container Variable: JSPORTS_USERNAME" Type="Variable" Display="advanced" Required="false" Mask="false">username</Config>
  <Config Name="Varible JSPORTS_PASSWORD" Target="JSPORTS_PASSWORD" Default="password" Mode="" Description="Container Variable: JSPORTS_PASSWORD" Type="Variable" Display="advanced" Required="false" Mask="false">password</Config>
  <Config Name="Varible JSPORTS_USE12HOUR" Target="JSPORTS_USE12HOUR" Default="yes" Mode="" Description="Container Variable: JSPORTS_USE12HOUR" Type="Variable" Display="advanced" Required="false" Mask="false">yes</Config>
</Container>
