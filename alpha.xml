<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>iptvboss-alpha</Name>
  <Beta>False</Beta>
  <Category>Tools:Utilities</Category>
  <Overview>iptvboss-alpha</Overview>
  <Description>
	iptvboss-alpha
  </Description>
  <Change></Change>
  <Support></Support>
  <Registry>https://registry.hub.docker.com/u/masterfwadmin/alpha/</Registry>
  <Repository>masterfwadmin/alpha</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>57000</HostPort> 
        <ContainerPort>5800</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
    </Networking>
    <Environment>
    <Variable>
      <Value>username</Value>
      <Name>JARVISSPORTS_USERNAME</Name>
      <Mode/>
      <Display>advanced-hide</Display>
    </Variable>
    <Variable>
      <Value>password</Value>
      <Name>JARVISSPORTS_PASSWORD</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>99</Value>
      <Name>USER_ID</Name>
      <Mode/>
      <Display>advanced</Display>
    </Variable>
    <Variable>
      <Value>100</Value>
      <Name>GROUP_ID</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/iptvboss-alpha</HostDir>
      <ContainerDir>/app</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:57000]/</WebUI>
  <Banner></Banner>
  <Icon>https://raw.githubusercontent.com/masterfwadmin/iptvboss/master/src/iptvboss.png</Icon>
    <!-- Variable Mappings -->
  <Config Name="User ID" Target="USER_ID" Default="99" Mode="" Description="ID of the user the application runs as." Type="Variable" Display="advanced-hide" Required="true" Mask="false"/>
  <Config Name="Group ID" Target="GROUP_ID" Default="100" Mode="" Description="ID of the group the application runs as." Type="Variable" Display="advanced-hide" Required="true" Mask="false"/>
  <Config Name="JarvisSports Username" Target="JARVISSPORTS_USERNAME" Default="" Mode="" Description="" Type="Variable" Display="advanced" Required="false" Mask="false"/>
</Container>
