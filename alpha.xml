<?xml version="1.0" encoding="utf-8"?>
<Container>
	<Name>iptvboss-alpha</Name>
	<Beta>False</Beta>
	<Category>Tools:Utilities</Category>
	<Overview>iptvboss-alpha</Overview>

	<Description>
		iptvboss-alpha
	</Description>
	<Change></Change>
	<Support></Support>
	<Registry>https://registry.hub.docker.com/u/masterfwadmin/alpha/</Registry>
	<Repository>masterfwadmin/alpha</Repository>
	<BindTime>true</BindTime>
	<Privileged>false</Privileged>
	<Networking>
		<Mode>bridge</Mode>
		<Publish>
		  <Port>
                    <HostPort>58001</HostPort>
                    <ContainerPort>5800</ContainerPort>
                    <Protocol>tcp</Protocol>
                  </Port>
		</Publish>
	</Networking>
	<Environment>
		<Variable></Variable>
	</Environment>
	<Data>
          <Volume>
            <HostDir>/mnt/cache/appdata/iptvboss</HostDir>
            <ContainerDir>/app</ContainerDir>
            <Mode>rw</Mode>
          </Volume>
        </Data>
	<WebUI>http://[IP]:[PORT:58001]/</WebUI>
	<Banner></Banner>
	<Icon>https://raw.githubusercontent.com/masterfwadmin/iptvboss/master/src/iptvboss.png</Icon>
</Container>
